package com.phq9_final.phq9final;

rule "PreScreenTree_0"
	when 
		PatientP : PatientParticular(marital_status matches "Separated")
	then 
		PreScreenR $var0 = new PreScreenR();
		$var0.setPreScreenResult( true );
		insert( $var0 );
end
